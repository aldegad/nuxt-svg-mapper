{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": false,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": false,
  "quoteProps": "preserve",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^path$",
    "^url$",
    "^vite$",
    "^vue$",
    "^vue",
    "^pinia",
    "^@tailwindcss/vite$",
    "^vitest",
    "^lodash",
    "^@iconify/vue",
    "^@aldegad/nuxt-core",
    "~svg-mapper",
    "^\\./",
    "../"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
